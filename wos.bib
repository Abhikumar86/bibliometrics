@article{ ISI:000257954000001,
Author = {Gnankambary, Z. and Bayala, J. and Malmer, A. and Nyberg, G. and Hien,
   V.},
Title = {{Decomposition and nutrient release from mixed plant litters of
   contrasting quality in an agroforestry parkland in the south-Sudanese
   zone of West Africa}},
