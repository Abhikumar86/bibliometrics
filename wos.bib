@article{ ISI:000457248700012,
Author = {Loria-Naranjo, Margarita and Sibaja-Cordero, Jeffrey A. and Cortes,
   Jorge},
Title = {{Mangrove Leaf Litter Decomposition in a Seasonal Tropical Environment}},
Journal = {{JOURNAL OF COASTAL RESEARCH}},
