@article{ ISI:000247638400015,
Author = {Mo, Jiangming and Brown, Sandra and Xue, Jinghua and Fang, Yunting and
   Li, Zhian and Li, Dejun and Dong, Shaofeng},
Title = {{Response of nutrient dynamics of decomposing pine (Pinus massoniana)
   needles to simulated N deposition in a disturbed and a rehabilitated
   forest in tropical China}},
Journal = {{ECOLOGICAL RESEARCH}},
Year = {{2007}},
Volume = {{22}},
Number = {{4}},
Pages = {{649-658}},
Month = {{JUL}},
Abstract = {{The effects of simulated N deposition on changes in mass, C, N and P of
   decomposing pine (Pinus massoniana) needles in a disturbed and a
   rehabilitated forest in tropical China were studied during a 24-month
   period. The objective of the study was to test the hypothesis that
   litter decomposition in a disturbed forest is more sensitive to N
   deposition rate than litter decomposition in a rehabilitated forest due
   to the relatively low nutrient status in the former as a result of
   constant human disturbance (harvesting understory and litter). The
   litterbag method and N treatments (control, no N addition; low-N, 5 g N
   m(-2) year(-1); medium-N, 10 g N m(-2) m(-1) were employed to evaluate
   decomposition. The results revealed that N addition increased (positive
   effect) mass loss rate and C release rate but suppressed (negative
   effect) the release rate of N and P from decomposing needles in both
   disturbed and rehabilitated forests. The enhanced needle decomposition
   rate by N addition was significantly related to the reduction in the C/N
   ratio in decomposing needles. However, N availability is not the sole
   factor limiting needle decomposition in both disturbed and rehabilitated
   forests. The positive effect was more sensitive to the N addition rate
   in the rehabilitated forest than in the disturbed forest, however the
   reverse was true for the negative effect. These results suggest that
   nutrient status could be one of the important factors in controlling the
   response of litter decomposition and its nutrient release to elevated N
   deposition in reforested ecosystems in the study region.}},
