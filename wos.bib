@article{ ISI:000256599700009,
Author = {Parsons, Scott A. and Congdon, Robert A.},
Title = {{Plant litter decomposition and nutrient cycling in north Queensland
   tropical rain-forest communities of differing successional status}},
Journal = {{JOURNAL OF TROPICAL ECOLOGY}},
Year = {{2008}},
