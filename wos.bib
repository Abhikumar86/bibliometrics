@article{ ISI:000308090000016,
Author = {Reed, Sasha C. and Townsend, Alan R. and Davidson, Eric A. and
   Cleveland, Cory C.},
Title = {{Stoichiometric patterns in foliar nutrient resorption across multiple
   scales}},
