author={Chen, H. and Dong, S. and Liu, L. and Ma, C. and Zhang, T. and Zhu, X. and Mo, J.},
title={Effects of experimental nitrogen and phosphorus addition on litter decomposition in an old-growth tropical forest},
journal={PLoS ONE},
year={2013},
volume={8},
