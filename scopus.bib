@ARTICLE{Zhou2018215,
author={Zhou, K. and Lu, X. and Mori, T. and Mao, Q. and Wang, C. and Zheng, M. and Mo, H. and Hou, E. and Mo, J.},
title={Effects of long-term nitrogen deposition on phosphorus leaching dynamics in a mature tropical forest},
journal={Biogeochemistry},
