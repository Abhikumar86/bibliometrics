@ARTICLE{Deng20188,
author={Deng, Q. and Zhang, D. and Han, X. and Chu, G. and Zhang, Q. and Hui, D.},
title={Changing rainfall frequency rather than drought rapidly alters annual soil respiration in a tropical forest},
journal={Soil Biology and Biochemistry},
year={2018},
volume={121},
pages={8-15},
doi={10.1016/j.soilbio.2018.02.023},
