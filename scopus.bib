@ARTICLE{Both20179307,
author={Both, S. and Elias, D.M.O. and Kritzler, U.H. and Ostle, N.J. and Johnson, D.},
title={Land use not litter quality is a stronger driver of decomposition in hyperdiverse tropical forest},
